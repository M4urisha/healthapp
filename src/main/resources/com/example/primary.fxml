<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="BASELINE_CENTER" prefHeight="540.0" prefWidth="270.0" spacing="10" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controllers.PrimaryController">
   <ImageView blendMode="MULTIPLY" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@images/Capture.PNG" />
      </image>
      <VBox.margin>
         <Insets top="25.0" />
      </VBox.margin>
   </ImageView>
   <Region prefHeight="129.0" prefWidth="270.0" />
    <Button fx:id="registerButton" onAction="#switchToSecondary" prefHeight="43.0" prefWidth="128.0" style="-fx-background-color: transparent;" styleClass="-fx-border-color: transparent;">
      <graphic>
         <ImageView fitHeight="67.0" fitWidth="138.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@images/Frame%209.png" />
            </image>
         </ImageView>
      </graphic></Button>
    <Button fx:id="loginButton" onAction="#switchToSecondary" prefHeight="28.0" prefWidth="66.0" style="-fx-background-color: transparent;" styleClass="-fx-border-color: transparent;" text="Login" textFill="#686666">
      <font>
         <Font name="Georgia" size="12.0" />
      </font></Button>
</VBox>
